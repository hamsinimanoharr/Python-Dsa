## demonstrating nested for loop
for i in range(3):
    for j in range(2):
        print(f"i:{i} and j:{j}")
## sum of n numbers using while and for loop
sum=0
count=1
n=10
while count<n:    
    sum=sum+count
    count=count+1
print("ans is",sum)

sum=0
count=1
n=10
for i in range(n):
    sum=i+sum
print(sum)
